# This file is sourced by Xsession(5), not executed.

if id $(whoami) | grep -q '(Domain Users)'; then
	KDEDIRS="/usr/share/bardinux-cajacanarias-default-settings/kde-profile/default/:$KDEDIRS"
fi
# Does not overlay Domain Guests profile.
export KDEDIRS
# Avoids persistent KDE cache to get work diverts of wallpaper.
export KDEVARTMP=/tmp
